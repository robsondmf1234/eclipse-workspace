
public class TesteConexao {

	public static void main(String[] args) {
			
		
		Conexao conn = null;
		try (Conexao con  = new Conexao()){
			conn = new Conexao();
			conn.leDados();
		} catch (IllegalStateException ex) {
			System.out.println("Deu erro na conexão.");
		}finally {
			System.out.println("Finally.");
			if (conn!= null) {
				conn.close();
			}
		}
		
		/*Conexao conn = null;
